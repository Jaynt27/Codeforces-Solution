#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        long long n,k;
        cin>>n>>k;
        vector<long long >arr(n,0);
        for(int i=0;i<n;i++){
            cin>>arr[i];
        }
        long long team=0;
        int idx=-1;
        sort(arr.begin(),arr.end(),greater<int>());
        for(int i=0;i<n;i++){
            if(arr[i]*(i-idx)>=k){
                team++;
                idx=i;
            }
        }
        cout<<team<<endl;
    }
    return 0;
}
